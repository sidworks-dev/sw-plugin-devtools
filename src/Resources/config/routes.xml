<?xml version="1.0" encoding="UTF-8" ?>
<routes xmlns="http://symfony.com/schema/routing"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://symfony.com/schema/routing
        https://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="frontend.action.sidworks-devtools.find-line" path="/_action/sidworks-devtools/find-line" methods="POST">
        <default key="_controller">Sidworks\DevTools\Controller\TemplateSearchController::findLineInTemplate</default>
        <default key="_routeScope">
            <list>
                <string>storefront</string>
            </list>
        </default>
    </route>
</routes>
